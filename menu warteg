    private void bhitungActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        int total = 0;
     if(cbRawon.isSelected()){
       String rawon = tfRawon.getText();
       int jumlahrawon = Integer.parseInt(rawon);
       int hargarawon  = jumlahrawon * 5000;
       total = total + hargarawon;
       }
     if(cbsoto.isSelected()){
       String soto = tfsoto.getText();
       int jumlahsoto = Integer.parseInt(soto);
       int hargaasoto = jumlahsoto * 6000;
       total = total + hargaasoto;
     }
     if(cbsate.isSelected()){
       String sate = Tfsate.getText();
       int jumlahsate = Integer.parseInt(sate);
       int hargasate = jumlahsate * 10000;
       total = total + hargasate;
     }
     if(cbgule.isSelected()){
       String gule = tfgule.getText();
       int jumlahgule = Integer.parseInt(gule);
       int hargagule = jumlahgule * 6000;
       total = total + hargagule;
     }
     if(cbkopi.isSelected()){
         String kopi = tfkopi.getText();
        int jumlahkopi =Integer.parseInt(kopi);
        int hargakopi = jumlahkopi * 2000;
       total = total + hargakopi;
     }
     if(cbesteh.isSelected()){
         String esteh = tfesteh.getText();
         int jumlahesteh = Integer.parseInt(esteh);
         int hargaesteh = jumlahesteh * 2000;
        total = total + hargaesteh;
     }
     if(cbjeruk.isSelected()){
         String jeruk =  tfesjeruk.getText();
         int jumlahjeruk = Integer.parseInt(jeruk);
         int hargajeruk = jumlahjeruk * 2500;
        total = total + hargajeruk;
        }
     if(cbescampur.isSelected()){
         String escampur = tfescampur.getText();
         int jumlahescampur =Integer.parseInt(escampur);
         int hargaescampur = jumlahescampur * 5000;
       total = total + jumlahescampur;
     }
     
         tfhitung.setText("" + total);
     
    }                                       
    
