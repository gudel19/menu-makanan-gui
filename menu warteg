    private void cbRawonActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
       if (cbRawon.isSelected() ==true ) {
           tfRawon.setEditable(b=true);
       }
       else {
           tfRawon.setEditable(b=false);
           tfRawon.setText("");
       }
        
        
    }                                       

